{
  "name": "remote-droid",
  "version": "0.1.6",
  "description": "OpenAPI to control your android devices",
  "author": "Uriel Chemouni <uchemouni@gmail.com> (https://urielch.github.io/)",
  "private": false,
  "license": "MIT",
  "keywords": [
    "adb",
    "adbkit",
    "android",
    "logcat",
    "monkey",
    "scrcpy",
    "minicap",
    "REST API"
  ],
  "bugs": {
    "url": "https://github.com/UrielCh/remote-droid/issues"
  },
  "main": "./dist/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/UrielCh/remote-droid.git"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "prepublish": "npm run build",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "_test:e2e": "dotenv -e .env.test -- jest --forceExit --detectOpenHandles --config ./test/jest-e2e.json",
    "_test:e2e:watch": "dotenv -e .env.test -- jest --watch --no-cache --config ./test/jest-e2e.json",
    "test:e2e": "run-s redis-test-up _test:e2e redis-test-down",
    "test:e2e:watch": "run-s redis-test-up _test:e2e:watch redis-test-down",
    "docker:build": "docker build --pull --rm -f Dockerfile -t urielch/remote-droid:latest .",
    "docker:debug": "docker run -it --rm urielch/remote-droid:latest /bin/sh",
    "docker:buildx": "docker buildx build --platform linux/amd64,linux/arm64 -t urielch/remote-droid:latest -t urielch/remote-droid:0.1.0 --push ."
  },
  "dependencies": {
    "@nestjs/common": "^9.0.11",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.11",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.11",
    "@nestjs/platform-ws": "^9.0.11",
    "@nestjs/swagger": "^6.1.2",
    "@nestjs/websockets": "^9.0.11",
    "@types/jest": "^28.1.8",
    "@u4/adbkit": "^4.1.10",
    "argon2": "^0.29.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "csv-reader": "^1.0.9",
    "express": "^4.18.1",
    "p-timeout": "4.1.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "picocolors": "^1.0.0",
    "promise-status-async": "^1.2.10",
    "redis": "^4.3.1",
    "redis-om": "^0.3.6",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.6",
    "sharp": "^0.30.7",
    "throttle-debounce": "4.0.0",
    "undici": "^5.10.0",
    "ws": "^8.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.1.3",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.0.11",
    "@types/express": "^4.17.13",
    "@types/node": "^18.7.17",
    "@types/passport-jwt": "^3.0.6",
    "@types/rimraf": "^3.0.2",
    "@types/sharp": "^0.30.5",
    "@types/supertest": "^2.0.12",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "28.1.3",
    "npm-run-all": "^4.1.5",
    "pactum": "^3.2.2",
    "prettier": "^2.7.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.4",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.8.3",
    "webpack": "^5.74.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "files": [
    "dist"
  ]
}