  "version": "0.2.0",
  "configurations": [
    {
      "name": "ts-node",
      "type": "node",
      "request": "launch",
      "args": [
        "${workspaceRoot}/wsCli.ts",
        "wg",
        "http://127.0.0.1:3009/remote/local/"
        // "http://10.22.0.240:3009/remote/local/"
      ],
      "runtimeArgs": [
        "-r",
        "ts-node/register",
      ],
      "cwd": "${workspaceRoot}",
      "protocol": "inspector",
      "internalConsoleOptions": "openOnSessionStart"
    }
  ]
}