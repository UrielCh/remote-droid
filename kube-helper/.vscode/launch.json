{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Generate Ingresses",
      "args": [
          "${workspaceFolder}/index.ts"
      ],
      "runtimeArgs": [
          "--nolazy",
          "-r",
          "ts-node/register",
          "-r",
          "tsconfig-paths/register"
      ],
      "sourceMaps": true,
      "cwd": "${workspaceRoot}",
      "console": "integratedTerminal",
      "env": {
        "APP_TAG_NAME": "app",
        "APP_TAG_VALUE": "remote-droid",
        "GENERATE_NAME": "remote-droid-",
        "INGRESS_NAME": "remote-droid-ingress",
        "POD_PORT": "3009",
      }
  },
  ]
}